@article{benetDisjoiningPressureHealing2014,
  title = {Disjoining {{Pressure}}, {{Healing Distance}}, and {{Film Height Dependent Surface Tension}} of {{Thin Wetting Films}}},
  author = {Benet, Jorge and Palanco, Jose G. and Sanz, Eduardo and MacDowell, Luis G.},
